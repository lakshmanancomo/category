{% block form %}
   
{{ parent() }}

<script type="text/javascript">

    function clickme(id){    
       var qid = id;
             
       var quantityValue =  document.getElementById (qid).value;
       
       var priceId = qid.replace('_quantity','_price');
       
       var priceValue = document.getElementById(priceId).value;
       
       var totalId = qid.replace('_quantity','_total');
        
       var totalValue = quantityValue * priceValue;
              
       document.getElementById(totalId).value=totalValue;
      
        
    
}



</script>
